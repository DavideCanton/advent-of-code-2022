{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Debug day",
      "type": "debugpy",
      "request": "launch",
      "program": "runner.py",
      "args": ["19", "1"],
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
        "name": "Python: Debug day custom input",
        "type": "debugpy",
        "request": "launch",
        "program": "runner.py",
        "args": ["16", "1", "-f", "foo.txt"],
        "console": "integratedTerminal",
        "justMyCode": true
      },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
